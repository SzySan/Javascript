<body>
    <div class="register-container">
        <h2>Rejestracja</h2>
        <form action="/register" method="post">
          
          <input type="text" placeholder="Podaj nazwę użytkownika" id="name" name="username" required /><br>

          <input type="password" placeholder="Podaj Hasło" id="password" name="password" required /><br>
  
          <button type="submit">Rejestracja</button><br>
        </form>
  
        <p>Masz już konto? <a href="/login">Login</a></p>
        <p>Powrót <a href="/">Strona Główna</a></p>
      </div>
      <script></script>
      <script src="index.js"></script>
      <script src="aplication.js"></script>
      
    </body>

</body>
